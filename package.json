{
  "name": "@googlemaps/react-wrapper",
  "version": "1.1.8",
  "description": "React component that wraps the loading of Google Maps JavaScript API.",
  "keywords": [
    "google",
    "maps",
    "react"
  ],
  "homepage": "https://github.com/googlemaps/react-wrapper",
  "bugs": {
    "url": "https://github.com/googlemaps/react-wrapper/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/googlemaps/react-wrapper.git"
  },
  "license": "Apache-2.0",
  "author": "Justin Poehnelt",
  "main": "dist/index.cjs.js",
  "browser": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "docs": "typedoc src/index.tsx",
    "format": "prettier *config.json *.js src/* --write && eslint src/* --fix",
    "lint": "eslint src/*",
    "prepare": "rollup -c",
    "test": "jest -i src/*",
    "test:e2e": "jest e2e/*",
    "build": "rm -rf dist && rollup -c && npm run build:examples",
    "build:examples": "rm -rf public && rollup -c rollup.config.examples.js"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.14.5",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.0",
    "@rollup/plugin-html": "^0.2.3",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^3.0.0",
    "@testing-library/dom": "^8.0.0",
    "@testing-library/react": "^12.0.0",
    "@types/google.maps": "^3.45.6",
    "@types/jest": "^27.0.1",
    "@types/react": ">=16.8.0",
    "@types/react-dom": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": ">=4.16.1",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.22.0",
    "jest": "^26.6.3",
    "prettier": "^2.0.4",
    "react": ">=16.8.0",
    "react-dom": "^17.0.1",
    "rollup": "^2.40.0",
    "rollup-plugin-filesize": "^9.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-jest": "^26.5.1",
    "tslib": "^2.1.0",
    "typedoc": "^0.22.4",
    "typescript": "^4.1.3"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://wombat-dressing-room.appspot.com"
  },
  "dependencies": {
    "@googlemaps/js-api-loader": "^1.11.1"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  }
}
